{
  "name": "@chzstream/extension",
  "version": "0.23.0",
  "type": "module",
  "author": "PortalCube",
  "license": "MIT",
  "scripts": {
    "dev": "wxt -p 5287",
    "dev:firefox": "wxt -b firefox -p 5288",
    "dev:edge": "wxt -b edge -p 5289",
    "dev:whale": "wxt -b whale -p 5290",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "build:whale": "wxt build -b whale",
    "build:all": "pnpm run build && pnpm run build:firefox && pnpm run build:edge && pnpm run build:whale",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "zip:whale": "wxt zip -b whale",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@chzstream/api": "workspace:^",
    "@chzstream/message": "workspace:^",
    "axios": "^1.8.2",
    "keyv": "^5.3.1",
    "nanoevents": "^9.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.308",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@wxt-dev/module-react": "^1.1.2",
    "typescript": "^5.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "wxt": "^0.19.29"
  }
}
